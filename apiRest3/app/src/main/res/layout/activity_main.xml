<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_dark"
    android:orientation="vertical"
    android:paddingBottom="5dp"
    android:paddingLeft="50dp"
    android:paddingRight="5dp"
    android:paddingTop="25dp"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tit_cidade"
        android:layout_width="60dp"
        android:layout_height="18dp"
        android:layout_above="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="12dp"
        android:layout_weight="0"
        android:text="Cidade:"
        tools:text="Cidade:" />

    <TextView
        android:id="@+id/randomUser"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:text="RandomUser"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/telefone"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/tit_telefone"
        android:layout_alignBottom="@+id/tit_telefone"
        android:layout_alignStart="@+id/nascimento"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/nascimento"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/tit_nascimento"
        android:layout_alignBottom="@+id/tit_nascimento"
        android:layout_alignStart="@+id/senha"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/senha"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/username"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/username"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/estado"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/estado"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/cidade"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/cidade"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/tit_cidade"
        android:layout_alignBottom="@+id/tit_cidade"
        android:layout_alignStart="@+id/endereco"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/endereco"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/tit_endereco"
        android:layout_alignBottom="@+id/tit_endereco"
        android:layout_alignStart="@+id/email"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/email"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignStart="@+id/sobrenome"
        android:layout_below="@+id/tit_sobrenome"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/nome"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignStart="@+id/sobrenome"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="50dp"
        android:layout_height="18dp"
        android:layout_above="@+id/tit_sobrenome"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="0dp"
        android:text="Nome:"
        tools:text="Nome:" />

    <TextView
        android:id="@+id/sobrenome"
        android:layout_width="300dp"
        android:layout_height="18dp"
        android:layout_alignBaseline="@+id/tit_sobrenome"
        android:layout_alignBottom="@+id/tit_sobrenome"
        android:layout_marginStart="16dp"
        android:layout_toEndOf="@+id/tit_sobrenome"
        android:layout_weight="0"
        android:text="Sobrenome:"
        tools:text="Sobrenome:" />

    <TextView
        android:id="@+id/tit_sobrenome"
        android:layout_width="100dp"
        android:layout_height="18dp"
        android:layout_above="@+id/tit_email"
        android:layout_alignEnd="@+id/tit_endereco"
        android:layout_marginBottom="25dp"
        android:text="Sobrenome:"
        tools:text="Sobrenome:" />

    <TextView
        android:id="@+id/tit_email"
        android:layout_width="60dp"
        android:layout_height="18dp"
        android:layout_alignEnd="@+id/textView20"
        android:layout_centerVertical="true"
        android:text="Email:"
        tools:text="Email:" />

    <TextView
        android:id="@+id/tit_endereco"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_above="@+id/tit_cidade"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="15dp"
        android:layout_weight="0"
        android:text="Endereco:"
        tools:text="Endereco:" />

    <TextView
        android:id="@+id/tit_nascimento"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_above="@+id/tit_telefone"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="17dp"
        android:layout_weight="0"
        android:text="Nascimento:"
        tools:text="Nascimento:" />

    <TextView
        android:id="@+id/tit_telefone"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="12dp"
        android:layout_weight="0"
        android:text="Telefone:"
        tools:text="Telefone:" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="60dp"
        android:layout_height="18dp"
        android:layout_above="@+id/tit_nascimento"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="15dp"
        android:layout_weight="0"
        android:text="Senha:"
        tools:text="Senha:" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="60dp"
        android:layout_height="18dp"
        android:layout_above="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="12dp"
        android:layout_weight="0"
        android:text="Username"
        tools:text="Username" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="60dp"
        android:layout_height="18dp"
        android:layout_above="@+id/textView7"
        android:layout_alignEnd="@+id/textView20"
        android:layout_marginBottom="11dp"
        android:text="Estado:"
        tools:text="Estado:" />

    <ImageView
        android:id="@+id/foto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="100dp"/>


</RelativeLayout>
