            </intent-filter>
        </service>

        <service
            android:name="studio.brunocasamassa.ajudaquiapp.MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
